@model IEnumerable<Kwizzotronic.Models.Quiz>

@{ ViewBag.Title = "MyQuizes"; }

<div class="mainHeader">
    Kwizzotronic
    <button class="backButton" onclick="history.go(-1); return false;">@Resources.Web_sitemap.back</button>
</div>

<table class="table">


    @foreach (var item in Model)
    {
<tr class="table-row">
    <td class="table-cell-group">
        @Html.DisplayFor(modelItem => item.Title)
    </td>
    <td class="table-cell-group">
        @Html.ActionLink(@Resources.Web_sitemap.start, "StartQuiz", new { quizId = item.IDQuiz }, new { @class = "btn btn-dark btn-cell" })
        @Html.ActionLink(@Resources.Web_sitemap.edit, "EditQuiz", new { quizId = item.IDQuiz }, new { @class = "btn btn-dark btn-cell" })
        @Html.ActionLink(@Resources.Web_sitemap.rankList, "GetRankingList", new { quizId = item.IDQuiz }, new { @class = "btn btn-dark btn-cell" })
    </td>
</tr>}

</table>